            "role": "Developer"
        },
        {
            "name": "Xavier Carriba",
            "homepage": "https://github.com/earelin/php-libkml",
            "role": "Developer of original version"
        }
    ],
    "autoload": {
        "psr-4": {
            "KML\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.1"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "3.*",
        "phpunit/phpunit": "^6.2"
    },
    "config": {
